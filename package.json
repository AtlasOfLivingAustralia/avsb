    "@tabler/icons-react": "^3.35.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dayjs": "^1.11.8",
    "embla-carousel": "^8.5.2",
    "embla-carousel-react": "^8.5.2",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "logrocket": "^4.0.2",
